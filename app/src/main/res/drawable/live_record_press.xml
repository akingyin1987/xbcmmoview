<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@drawable/live_record_active"/>
    <item android:state_pressed="false" android:drawable="@drawable/live_record"/>
    <item android:state_selected="true" android:drawable="@drawable/live_record_active"/>
    <item android:state_selected="false" android:drawable="@drawable/live_record"/>
    <item android:drawable="@drawable/live_record_active" android:state_activated="true"/>
    <item android:drawable="@drawable/live_record" android:state_activated="false"/>
</selector>
